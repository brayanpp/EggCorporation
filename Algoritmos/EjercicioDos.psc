Algoritmo EjercicioDos
	
	Definir Base, altura, area, perimetro Como Real;
	Escribir "Por favor ingresa la base del rectangulo en CM";	
	Leer base;
	
	Escribir "Por favor ingresa la altura del rectangula en CM";
	Leer altura;
	
	area = base*altura;
	perimetro = 2*altura + 2 * base;
	
	Imprimir  "El area del rectangulo es: ", area "cm";
	Imprimir  "El perimetro del rectangulo es ", perimetro "cm";
	
FinAlgoritmo
